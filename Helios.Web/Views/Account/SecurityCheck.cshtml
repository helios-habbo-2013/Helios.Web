<html>
<head>
    <title>Redirecting...</title>
    <meta http-equiv="content-type" content="text/html" />
    <style type="text/css">
        body {
            background-color: #e3e3db;
            text-align: center;
            font: 11px Verdana, Arial, Helvetica, sans-serif;
        }

        a {
            color: #fc6204;
        }</style>
</head>
<body>

    <script type="text/javascript">window.location.replace('@(this.ViewBag.StaticContentUrl)@(this.ViewBag.RedirectPath)');</script>
    <noscript><meta http-equiv="Refresh" content="0;URL=@(this.ViewBag.StaticContentUrl)@(this.ViewBag.RedirectPath)"></noscript>

    <p class="btn">If you are not automatically redirected, please <a href="@(this.ViewBag.StaticContentUrl)@(this.ViewBag.RedirectPath)" id="manual_redirect_link">click here</a></p>


</body>
</html>
