<!--[if lt IE 7]>
<link rel="stylesheet" href="@(this.ViewBag.StaticContentUrl)web-gallery/static/styles/ie6.css" />
<script src="@(this.ViewBag.StaticContentUrl)web-gallery/static/js/pngfix.js" type="text/javascript"></script>
<style type="text/css">
    body { behavior: url(/js/csshover.htc); }
</style>
<![endif]-->
   </head>
   <body id="me" class="@(ViewBag.LoggedIn ? "" : " anonymous")">
    @{
        await Html.RenderPartialAsync("Navigation");
    }
    <div id="container">
        <div id="content"  style="position: relative" class="clearfix">
            <div id="column1" class="column">
                <!-- <div class="rounded" style="background-color: red; color: white">
                <strong>Attention!</strong><br />
                This server is currently in beta. That means there may be some incomplete features, bugs and other forms of exploitation.<br />
                </div>
                <br /> -->
                @{
                    var column1Hablets = hablets.Where(x => x.Item2 == "column1").Select(x => x.Item1).ToList();

                    foreach (var hablet in column1Hablets) 
                    {
                        await Html.RenderPartialAsync("Habblets/" + hablet);
                    }
                }
         </div>
         <div id="column2" class="column">
                 @{
                    var column2Hablets = hablets.Where(x => x.Item2 == "column2").Select(x => x.Item1).ToList();

                    foreach (var hablet in column2Hablets) 
                    {
                        await Html.RenderPartialAsync("Habblets/" + hablet);
                    }
                }
         </div>
         <script type="text/javascript">
            HabboView.run();
         </script>
         <div id="column3" class="column">
            <div class="habblet-container ">
               <div class="ad-container">
                  <br>
               </div>
            </div>
            <script type="text/javascript">if (!$(document.body).hasClassName('process-template')) { Rounder.init(); }</script>
         </div>
         <!--[if lt IE 7]>
         <script type="text/javascript">
            Pngfix.doPngImageFix();
         </script>
         <![endif]-->
      </div>
      </div>
      @{
      await Html.RenderPartialAsync("Footer");
      }
   </body>
</html>